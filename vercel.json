{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel_deploy.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel_deploy.py"
        }
    ]
}