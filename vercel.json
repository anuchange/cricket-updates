{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel_deploy.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel_deploy.py"
        }
    ],
    "functions": {
        "/api/cron-jobs": {
            "maxDuration": 60
        }
    },
    "crons": [
        {
            "path": "/api/cron-jobs",
            "schedule": "0 16 * * *"
        }
    ]
}
