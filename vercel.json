{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel_deploy.py",
            "use": "@vercel/python@3.1.32",
            "config": { 
                "runtime": "python3.11",
                "pythonVersion": "3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel_deploy.py"
        }
    ]
}