{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel_deploy.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel_deploy.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron-jobs",
            "schedule": "0 16 * * *"
        }
    ]
}