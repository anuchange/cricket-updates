{
    "version": 2,
    "functions": {
        "src/**/*": {
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel_deploy.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron-jobs",
            "schedule": "0 16 * * *"
        }
    ]
}